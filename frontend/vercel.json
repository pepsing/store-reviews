{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://43.156.243.177:8001/:path*"
    },
    {
      "source": "/config/:path*",
      "destination": "/config/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "http://43.156.243.177:8001"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "http://43.156.243.177:8001"
    }
  },
  "buildCommand": "NODE_OPTIONS=--openssl-legacy-provider craco build",
  "outputDirectory": "build"
} 